{
    "name": "unmous/core_plugin",
    "type": "digraph-plugin",
    "require": {
        "digraphcms/digraph-core": "^2",
        "apereo/phpcas": "^1.6",
        "mpdf/qrcode": "^1.2"
    },
    "require-dev": {
        "php": ">=8.1",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DigraphCMS_Plugins\\unmous\\ous_digraph_module\\": "src/"
        }
    },
    "scripts": {
        "migrate": [
            "phinx migrate"
        ],
        "seed": [
            "phinx seed:run"
        ]
    }
}
